{
   "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "suppressImplicitAnyIndexErrors": true,
      "lib": ["es5", "es6", "dom"],
      "noEmit": true
   },
   "include": ["./src/**/*"]
}
